{
  "manifest_version": 3,
  "name": "UpdatedBookmarks : bookmarks updater",
  "version": "1.0.2",
  "description": "Keeps bookmarks updated inside the UpdatedBookmarks folder.",
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "UpdatedBookmarks"
  },
  "icons": {
    "128": "128-icon.png"
  }
}