{
  "manifest_version": 2,
  "name": "UpdatedBookmarks bookmarks manager",
  "version": "1.0",
  "description": "Keeps bookmarks updated inside the UpdatedBookmarks folder.",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ScanFolder Favorites"
  }
}